{
  "expo": {
    "name": "MuscleMetric",
    "slug": "musclemetric",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "musclemetric",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.musclemetric.app",
      "infoPlist": {
        "NSMotionUsageDescription": "We use your motion data to show daily step counts.",
        "NSSupportsLiveActivities": true,
        "NSSupportsLiveActivitiesFrequentUpdates": false,
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.musclemetric.app",
      "permissions": [
        "ACTIVITY_RECOGNITION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-live-activity",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": false,
      "reactCompiler": false
    },
    "extra": {
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "LiveActivity",
                  "bundleIdentifier": "com.musclemetric.app.LiveActivity",
                  "entitlements": {}
                }
              ]
            }
          }
        },
        "projectId": "1e89dfbc-feae-4387-82bd-a1d3252bd76d"
      },
      "router": {}
    }
  }
}
